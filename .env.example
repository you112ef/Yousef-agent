# Yousef Agent - Environment Variables Configuration
# Copy this file to .env.local and fill in your actual values

# =============================================================================
# AUTHENTICATION & SESSION
# =============================================================================
AUTH_SECRET=your-random-secret-key-here-min-32-characters
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# GitHub OAuth (for sign in)
GITHUB_CLIENT_ID=your-github-oauth-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-client-secret

# Vercel OAuth (for sign in)
VERCEL_CLIENT_ID=your-vercel-oauth-client-id
VERCEL_CLIENT_SECRET=your-vercel-oauth-client-secret

# =============================================================================
# DATABASE
# =============================================================================
DATABASE_URL=your-database-connection-string
# For Neon/PostgreSQL: postgresql://username:password@host:port/database
# For local development: postgresql://user:password@localhost:5432/dbname

# =============================================================================
# API KEYS FOR AI AGENTS
# =============================================================================

# Claude (Anthropic) - Required for Claude agent
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# OpenAI - Required for Codex, OpenCode agents
OPENAI_API_KEY=sk-your-openai-api-key-here

# Gemini (Google) - Required for Gemini agent
GEMINI_API_KEY=your-gemini-api-key-here

# Cursor AI - Required for Cursor agent
CURSOR_API_KEY=your-cursor-api-key-here

# OpenRouter - Required for Cline and Kilo agents
OPENROUTER_API_KEY=sk-or-your-openrouter-api-key-here

# Vercel AI Gateway - Alternative to direct OpenAI API
AI_GATEWAY_API_KEY=your-ai-gateway-api-key-here

# =============================================================================
# SANDBOX & EXECUTION
# =============================================================================
# Vercel Sandbox Configuration (for code execution)
SANDBOX_VERCEL_TOKEN=your-vercel-sandbox-token
SANDBOX_VERCEL_TEAM_ID=your-sandbox-team-id
SANDBOX_VERCEL_PROJECT_ID=your-sandbox-project-id

# Default maximum sandbox duration in minutes
MAX_SANDBOX_DURATION=300

# =============================================================================
# VERCEL CONFIGURATION
# =============================================================================
# For Vercel deployment
VERCEL_TOKEN=your-vercel-token
VERCEL_PROJECT_ID=your-vercel-project-id
VERCEL_ORG_ID=your-vercel-org-id

# =============================================================================
# ADDITIONAL SERVICES
# =============================================================================
# GitHub Personal Access Token (for private repo access)
GITHUB_TOKEN=ghp_your-github-personal-access-token

# Git credentials for commits
GIT_AUTHOR_NAME=Yousef Agent
GIT_AUTHOR_EMAIL=agent@example.com

# =============================================================================
# RATE LIMITING & SECURITY
# =============================================================================
# Rate limiting configuration
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# =============================================================================
# MONITORING & ANALYTICS
# =============================================================================
# Vercel Analytics (automatically enabled)
# No additional configuration needed

# Sentry Error Tracking (optional)
# SENTRY_DSN=your-sentry-dsn-here

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================
# Set to 'development' for local development, 'production' for live
NODE_ENV=development

# Debug logging
DEBUG=false
LOG_LEVEL=info

# =============================================================================
# NOTES
# =============================================================================
# 1. Generate random secrets using: openssl rand -base64 32
# 2. Get API keys from respective service providers
# 3. For GitHub OAuth: https://github.com/settings/developers
# 4. For Vercel OAuth: https://vercel.com/account/tokens
# 5. For database: Use Neon, Supabase, or local PostgreSQL
# 6. For sandbox: Sign up at https://vercel.com/products/sandbox
#
# SECURITY WARNING:
# - Never commit .env.local to version control
# - Use different secrets for each environment
# - Regularly rotate API keys and secrets
